# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  wndw_config = debug
endif
ifeq ($(config),release)
  wndw_config = release
endif
ifeq ($(config),dist)
  wndw_config = dist
endif

PROJECTS := wndw

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

wndw:
ifneq (,$(wndw_config))
	@echo "==== Building wndw ($(wndw_config)) ===="
	@${MAKE} --no-print-directory -C . -f wndw.make config=$(wndw_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f wndw.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo "  dist"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   wndw"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"